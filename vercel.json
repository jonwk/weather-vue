{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/client.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        }
    ]
}